<h1>会員情報編集</h1>

<%= form_with model: @customer do |f| %>
<div>
  <label for="inputName">氏名</label>
  <%= f.text_field :last_name, autofocus: true, id:"inputName", placeholder:"姓"%> <%= f.text_field :first_name, autofocus: true, id:"inputName", placeholder:"名"%>
</div>
<div>
  <%= f.submit "編集内容を保存" %>
</div>
<% end %>